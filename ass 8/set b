2.

bcafy64=# create table dept(dno int primary key,dname varchar(30),loc varchar(30));
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "dept_pkey" for table "dept"
CREATE TABLE
bcafy64=# \d dept
            Table "public.dept"
 Column |         Type          | Modifiers 
--------+-----------------------+-----------
 dno    | integer               | not null
 dname  | character varying(30) | 
 loc    | character varying(30) | 
Indexes:
    "dept_pkey" PRIMARY KEY, btree (dno)

***********************************************************************************************************************************************************


bcafy64=# create table emp(eno int primary key,ename varchar(30),desg varchar(20),salary float,dno int references dept(dno) on delete cascade on update cascade);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "emp_pkey" for table "emp"
CREATE TABLE
bcafy64=# \d emp
             Table "public.emp"
 Column |         Type          | Modifiers 
--------+-----------------------+-----------
 eno    | integer               | not null
 ename  | character varying(30) | 
 desg   | character varying(20) | 
 salary | double precision      | 
 dno    | integer               | 
Indexes:
    "emp_pkey" PRIMARY KEY, btree (eno)
Foreign-key constraints:
    "emp_dno_fkey" FOREIGN KEY (dno) REFERENCES dept(dno) ON UPDATE CASCADE ON DELETE CASCADE

***************************************************************************************************************************************************************
bcafy64=# select * from dept;
 dno |    dname    |     loc     
-----+-------------+-------------
   2 | sales       | firstfloor
   3 | datanalysis | secondfloor
   4 | legal       | thirdfloor
   5 | management  | forthfloor
(4 rows)

*****************************************************************************************************************************************************************
bcafy64=# \d emp
             Table "public.emp"
 Column |         Type          | Modifiers 
--------+-----------------------+-----------
 eno    | integer               | not null
 ename  | character varying(30) | 
 desg   | character varying(20) | 
 salary | double precision      | 
 dno    | integer               | 
Indexes:
    "emp_pkey" PRIMARY KEY, btree (eno)
Foreign-key constraints:
    "emp_dno_fkey" FOREIGN KEY (dno) REFERENCES dept(dno) ON UPDATE CASCADE ON DELETE CASCADE

