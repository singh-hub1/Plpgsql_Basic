bcafy64=# create table owner(oname varchar(20) primary key ,address varchar(50),phone int);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "owner_pkey" for table "owner"
CREATE TABLE
bcafy64=# \d owner
            Table "public.owner"
 Column  |         Type          | Modifiers 
---------+-----------------------+-----------
 oname   | character varying(20) | not null
 address | character varying(50) | 
 phone   | integer               | 
Indexes:
    "owner_pkey" PRIMARY KEY, btree (oname)

**************************************************************************************************************************************************

bcafy64=# create table property_1(pno int primary key,descriptiom text,area varchar(30),oname varchar(20) references owner(oname) on delete cascade on update cascade);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "property_1_pkey" for table "property_1"
CREATE TABLE
bcafy64=# \d property_1
            Table "public.property_1"
   Column    |         Type          | Modifiers 
-------------+-----------------------+-----------
 pno         | integer               | not null
 descriptiom | text                  | 
 area        | character varying(30) | 
 oname       | character varying(20) | 
Indexes:
    "property_1_pkey" PRIMARY KEY, btree (pno)
Foreign-key constraints:
    "property_1_oname_fkey" FOREIGN KEY (oname) REFERENCES owner(oname) ON UPDATE CASCADE ON DELETE CASCADE

***************************************************************************************************************************************************

