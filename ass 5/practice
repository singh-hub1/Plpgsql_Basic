bcafy2=# create table emp(eno int primary key,ename varchar(50),salaray float);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "emp_pkey" for table "emp"
CREATE TABLE
bcafy2=# \d emp
             Table "public.emp"
 Column  |         Type          | Modifiers 
---------+-----------------------+-----------
 eno     | integer               | not null
 ename   | character varying(50) | 
 salaray | double precision      | 
Indexes:
    "emp_pkey" PRIMARY KEY, btree (eno)

bcafy2=# create table books(id int unique,title text not null,author_id int,sub_id integer,constraint books_id primary key(id));
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "books_id" for table "books"
CREATE TABLE
bcafy2=# \d books
      Table "public.books"
  Column   |  Type   | Modifiers 
-----------+---------+-----------
 id        | integer | not null
 title     | text    | not null
 author_id | integer | 
 sub_id    | integer | 
Indexes:
    "books_id" PRIMARY KEY, btree (id)


                                                                 
bcafy2=# create table sales_order(order_no char(10) primary key,order_date date,salesman_no int);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "sales_order_pkey" for table "sales_order"
CREATE TABLE
bcafy2=# \d sales_order
       Table "public.sales_order"
   Column    |     Type      | Modifiers 
-------------+---------------+-----------
 order_no    | character(10) | not null
 order_date  | date          | 
 salesman_no | integer       | 
Indexes:
    "sales_order_pkey" PRIMARY KEY, btree (order_no)


bcafy2=# create table client_master(client_no int constraint p_client primary key,name varchar(50),addr text,bal_due int);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "p_client" for table "client_master"
CREATE TABLE
bcafy2=# \d client_master
         Table "public.client_master"
  Column   |         Type          | Modifiers 
-----------+-----------------------+-----------
 client_no | integer               | not null
 name      | character varying(50) | 
 addr      | text                  | 
 bal_due   | integer               | 
Indexes:
    "p_client" PRIMARY KEY, btree (client_no)

bcafy2=# create table inventory(inv_no int primary key,in_stock boolean);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "inventory_pkey" for table "inventory"
CREATE TABLE
bcafy2=# \d inventory
    Table "public.inventory"
  Column  |  Type   | Modifiers 
----------+---------+-----------
 inv_no   | integer | not null
 in_stock | boolean | 
Indexes:
    "inventory_pkey" PRIMARY KEY, btree (inv_no)


bcafy2=# create table sales_order1(order_no char(10),product_no char(10),qty_order int,product_rate numeric(8,2),primary key(order_no,product_no));
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "sales_order1_pkey" for table "sales_order1"
CREATE TABLE
bcafy2=# \d sales_order1
       Table "public.sales_order1"
    Column    |     Type      | Modifiers 
--------------+---------------+-----------
 order_no     | character(10) | not null
 product_no   | character(10) | not null
 qty_order    | integer       | 
 product_rate | numeric(8,2)  | 
Indexes:
    "sales_order1_pkey" PRIMARY KEY, btree (order_no, product_no)

